{
  "name": "papos",
  "version": "0.0.1",
  "description": "Platform Operations",
  "type": "module",
  "scripts": {
    "build": "unbuild",
    "play:nuxt": "pnpm run -r --filter=./playground/nuxt dev",
    "play:vite": "pnpm run -r --filter=./playground/vite dev",
    "publish": "pnpm build && npm publish",
    "lint": "eslint --fix"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/apapos/papos"
  },
  "exports": {
    "./shared": {
      "types": "./packages/shared/dist/index.d.ts",
      "import": "./packages/shared/dist/index.mjs"
    },
    "./utils": {
      "types": "./packages/utils/dist/index.d.ts",
      "import": "./packages/utils/dist/index.mjs"
    }
  },
  "files": [
    "packages/shared/dist",
    "packages/utils/dist"
  ],
  "keywords": [
    "lib",
    "common"
  ],
  "author": "apapos",
  "license": "MIT",
  "packageManager": "pnpm@9.10.0",
  "devDependencies": {
    "eslint": "^8.57.0",
    "unbuild": "^2.0.0"
  }
}
